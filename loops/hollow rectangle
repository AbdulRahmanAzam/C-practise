#include<stdio.h>

int main() {
    int i=0, j =0, n =3,m;
    printf("Enter column then row ");
    scanf("%d",&n);
    
    for(i= 1; i<= n; i++){
        for(j =1; j<= n; j++){
            if(j == 1 || j ==n || i == 1 || i ==n){
                printf("*");
            }
            else{
                printf(" ");
            }
        }
        printf("\n");
    }
}//end main
