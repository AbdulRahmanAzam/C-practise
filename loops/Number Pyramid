## Discription:
n = 4
   1
  123
 12345
1234567

## Code:

#include <stdio.h>
int main() {
    int i, j,k, a, n;
    printf(" n = ");
    scanf("%d", &n);
    a=1;
    for(i =1; i <= n; i++){
        for(j =1; j <=n-i ; j++){
            printf(" ");
        }//end j loop
        for(k=1; k <= a; k++){
            printf("%d",k);
        }
        a = a+2;
        printf("\n");
    }// end i loop

    return 0;
}//end main
