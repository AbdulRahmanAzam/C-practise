#include<stdio.h>

int main() {
    int i=0, j =0, n =3, a=65;
    printf("Enter a number ");
    scanf("%d", &n);
    
    for(i=1; i<= n; i++){
        if(i % 2 == 0){
            for(j=1;j<=i;j++){
            printf("%c",a);
            a= a+1;
            }
            a=65;
        }//if for loop alphabet
        else{
            for(j=1;j<=i; j++){
                printf("%d",j);
            }
        }
        printf("\n");
    }
  }//end main
