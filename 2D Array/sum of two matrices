## Discirption:
adding two matrices

#include <stdio.h>
int main(){
    int a,b,c,i,j,k,l,n;
    int arr[2][2] = {{1,2},
                    {3,4}};
    int brr[2][2] = {{5,6},
                    {7,8}};
    int summ[2][2] ={0};
    
    for(i=0;i<2;i++){
        for(j=0;j<2;j++){
            summ[i][j] = arr[i][j] + brr[i][j];
        }
    }
    for(k=0;k<2;k++){
        for(l=0;l<2;l++){
            printf("%d ", summ[k][l]);
        }
        printf("\n");
    }
}//end main
