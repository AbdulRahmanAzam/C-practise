
#include <stdio.h>

int main() {
    int arr[5] = {3,2,4,1,5};
    int i, a = arr[0], min= arr[0], secmin = 0, max = arr[i], secmax=0;
    for(i=0;i<5;i++){
        if(min > arr[i] ){
            secmin = min;
            min = arr[i];
        }
        if(max < arr[i]){
            secmax = max;
            max = arr[i];
        }
    }//end for loop
    printf("min %d \nsecmin = %d \n max= %d\n secmax = %d", min, secmin, max, secmax);

    return 0;
}//end main
