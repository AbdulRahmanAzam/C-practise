#include <stdio.h>
void structure(int arr[3][3]){
    int a=1,b,c;
    for(int i =0; i<3; i++){
        for(int j =0; j<3;j++){
            arr[i][j] = a++;
        }
    }
}
void output(int arr[3][3]){
    int a=1,b,c;
    for(int i =0; i<3; i++){
        for(int j =0; j<3;j++){
            printf(" %d ",arr[i][j]);
            if(j<2){
                printf("|");
            }
        }
        printf("\n");
        for(int j =0; j<3;j++){
            if(i<2)
            printf(" -- ");
        }
        printf("\n");
    }
}

